<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Express Skills</title>
</head>
<body>
  <h1>Skills</h1>
  <a href="/skills/new">Add New Skill</a>
  <ul>
    <% skills.forEach(function(a) { %>
      <li>
        <a href="/skills/<%= a.id %>"><%= a.skill %></a>
          - 
        <%= a.done ? 'done' : 'not done' %>
      </li>
  </ul>

    
  </body>
</html>
